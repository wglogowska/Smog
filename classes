class rectangle:
    def __init__(self,width,height):
    
        self.width=width
        self.height=height
        
    def area_rct(self):
        
        return self.width*self.height
    
p=rectangle(2,5)
area_rct=p.area_rct()
    
class triangle:
    def __init__(self,width,height):
        
        self.width=width
        self.height=height
    def area_tri(self):
        
        return 1/2*self.width*self.height
p=triangle(3,2)
area_tri=p.area_tri()
